<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.*?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Searchsalaryrange">
   <children>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="600.0" style="-fx-background-color: blue;">
         <children>
            <Label text="Give the Salary Range" textFill="#f70505">
               <font>
                  <Font size="36.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <Label layoutX="114.0" layoutY="118.0" style="-fx-background-color: #91903f;" text="Lower Bound" textFill="#1100ff">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label layoutX="113.0" layoutY="177.0" style="-fx-background-color: #91903f;" text="Upper Bound" textFill="#1100ff">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="lowerbound" layoutX="240.0" layoutY="114.0" prefHeight="35.0" prefWidth="150.0">
         <font>
            <Font size="14.0" />
         </font></TextField>
      <TextField fx:id="upperbound" layoutX="240.0" layoutY="173.0" prefHeight="35.0" prefWidth="150.0">
         <font>
            <Font size="14.0" />
         </font></TextField>
      <Button fx:id="search" layoutX="401.0" layoutY="248.0" mnemonicParsing="false" onAction="#search" prefHeight="40.0" prefWidth="90.0" style="-fx-background-color: #0a0103;" text="Search" textFill="#00ff33">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="back" layoutX="275.0" layoutY="248.0" mnemonicParsing="false" onAction="#back" prefHeight="40.0" prefWidth="90.0" style="-fx-background-color: black;" text="Back" textFill="#ec031a">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
